{
  "name": "ReactApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build-android": "react-native bundle --entry-file ./index.js --platform android --dev false --bundle-output ./android/app/src/main/assets/main.jsbundle --sourcemap-output ./android/app/src/main/assets/main.jsbundle.map --assets-dest ./android/app/src/main/res --reset-cache --verbose",
    "build-ios": "react-native bundle --entry-file ./index.js --platform ios --dev false --bundle-output ./ios/ReactApp/React/main.jsbundle --sourcemap-output ./ios/ReactApp/React/main.jsbundle.map --assets-dest ./ios/ReactApp/React --reset-cache --verbose",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.2.0",
    "react-native": "0.53.0",
    "react-navigation": "^1.3.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-jest": "22.4.1",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.4.2",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
